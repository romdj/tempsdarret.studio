{
  "name": "@tempsdarret/models",
  "version": "0.1.0",
  "description": "TypeSpec models and API contracts for photography platform",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsp compile .",
    "dev": "tsp compile . --watch",
    "clean": "rm -rf dist tsp-output",
    "validate": "tsp compile . --no-emit"
  },
  "devDependencies": {
    "@typespec/compiler": "workspace:^",
    "@typespec/library-linter": "workspace:^",
    "@typespec/streams": "workspace:^",
    "@typespec/tspd": "workspace:^",
    "@typespec-tools/emitter-typescript": "^0.3.0"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  }
}
